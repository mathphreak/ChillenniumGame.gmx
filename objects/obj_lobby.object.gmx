            <string>// Do we have a valid number of players?
var players = net_getPlayers();
var playerCount = array_height_2d(players);
var allReady = net_allPlayersReady();

var canStart = (playerCount == 3 || playerCount == 5) &amp;&amp; allReady;

if (canStart) {
    draw_set_alpha(1);
} else {
    draw_set_alpha(0.5);
}

//Draw menu stuff
draw_set_font(fnt_menus)
draw_text(room_width/2, room_height*.59, "Start Game");
draw_rectangle(375,515,520,550, true);

//Player clicks start game
if (canStart &amp;&amp; point_in_rectangle(mouse_x, mouse_y, 375,515,520,550)){
    if(mouse_check_button_pressed(mb_left)){
        net_distributeRoles();
        room_goto(rm_game)//add newgame object to nettestroom
        instance_destroy();
    }
}

draw_set_alpha(1);

</string>
